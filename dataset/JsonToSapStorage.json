			"NomeFile": {
				"type": "string",
				"defaultValue": "default.json"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().NomeFile",
					"type": "Expression"
				},
				"container": "sap"
			}
		},
		"schema": {
			"type": "object",
			"properties": {
				"numero": {
					"type": "integer"
				},
				"dataFattura": {
