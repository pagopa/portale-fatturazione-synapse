					"type": "string"
				},
				"tipologiaFattura": {
					"type": "string"
				},
				"istitutioID": {
					"type": "string"
				},
				"onboardingTokenID": {
					"type": "string"
				},
				"tipoDocumento": {
					"type": "string"
				},
				"divisa": {
					"type": "string"
				},
				"metodoPagamento": {
					"type": "string"
				},
				"causale": {
					"type": "string"
				},
				"split": {
					"type": "boolean"
				},
				"sollecito": {
					"type": "string"
				},
				"datiGeneraliDocumento": {
					"type": "array",
