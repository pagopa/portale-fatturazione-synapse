{
	"name": "SendJsonToSap",
	"properties": {
		"activities": [
			{
				"name": "Generate Json",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "JsonToSAP",
						"type": "DataFlowReference",
						"parameters": {
							"AnnoRiferimento": {
								"value": "@pipeline().parameters.AnnoRiferimento",
								"type": "Expression"
							},
							"MeseRiferimento": {
								"value": "@pipeline().parameters.MeseRiferimento",
								"type": "Expression"
							},
							"TipologiaFattura": {
								"value": "'@{pipeline().parameters.TipologiaFattura}'",
								"type": "Expression"
							}
						},
						"datasetParameters": {
							"ExportJsonToBlobStorage": {
								"NomeFile": "FatturePagoPa.json"
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "None",
					"cacheSinks": {
						"firstRowOnly": true
					}
				}
			}
		],
		"parameters": {
			"AnnoRiferimento": {
				"type": "int"
			},
			"MeseRiferimento": {
				"type": "int"
			},
			"TipologiaFattura": {
				"type": "string"
			}
		},
		"folder": {
			"name": "ManageInvoice"
		},
		"annotations": []
	}
}